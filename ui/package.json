{
  "name": "nf-workflow-ui",
  "version": "1.0.0",
  "description": "Workflow User Interface",
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "server": "nodemon server/index.js",
    "build": "(true | rm -rf ./dist) && cp -r server dist && npm run build-ui && cp -r build dist/public",
    "build-ui": "react-scripts build",
    "deploy": "",
    "eslint": "eslint  [src]*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Netflix/conductor.git"
  },
  "keywords": [
    "Workflow",
    "conductor"
  ],
  "license": "SEE LICENSE IN license.txt",
  "dependencies": {
    "axios": "^0.18.0",
    "bunyan": "^1.7.1",
    "clipboard": "^1.6.1",
    "d3": "^3.5.16",
    "dagre-d3": "^0.4.17",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "lodash": "^4.0.0",
    "lodash.merge": "^4.3.0",
    "moment": "^2.17.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-typeahead": "^0.9.2",
    "react-d3-basic": "^1.5.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "mocha": "^2.5.3",
    "nodemon": "^1.18.3",
    "react-scripts": "^1.1.4"
  }
}
