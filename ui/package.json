{
  "name": "nf-workflow-ui",
  "version": "1.0.0",
  "description": "Workflow User Interface",
  "proxy": {
    "/api": {
      "target": "http://localhost:5000"
    }
  },
  "scripts": {
    "test": "react-scripts test",
    "start": "react-scripts start",
    "deploy": "react-scripts build && (rm -rf ./server/public/build | true) && cp -r build server/public",
    "build": "react-scripts build",
    "server": "nodemon ./server/server.js",
    "install:multi-python": "npm i --python=python2.7",
    "watch": "npm-run-all --parallel server start"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Netflix/conductor.git"
  },
  "keywords": [
    "Workflow",
    "conductor"
  ],
  "license": "SEE LICENSE IN license.txt",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "3.3.6",
    "clipboard": "^1.6.1",
    "d3": "^3.5.16",
    "dagre-d3": "^0.4.17",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "lodash": "^4.0.0",
    "moment": "^2.17.1",
    "prop-types": "^15.5.9",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.3",
    "react-bootstrap-table": "^4.3.1",
    "react-bootstrap-typeahead": "^3.2.2",
    "react-dom": "^16.4.2",
    "react-redux": "^4.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^3.3.1",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^1.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "react-scripts": "^1.1.5",
    "webpack": "^3.10.0"
  }
}
