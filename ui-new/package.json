{
  "name": "conductor-ui",
  "version": "0.0.1",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "server": "JAVA_API_BASE_URL=http://localhost:8080 BASE_URL=/api/v1 PORT=3001 IP=localhost nodemon server/index.js",
    "build": "react-scripts build && rm -rf ./server/public | true && mv ./build ./server/public",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "material-ui": "^1.0.0-beta.47",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-js-diagrams": "^3.1.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "nodemon": "^1.18.3"
  }
}
